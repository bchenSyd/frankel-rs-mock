    "casual": "^1.5.12",
    "cors": "^2.8.3",
    "express": "^4.15.3",
    "express-graphql": "^0.6.5",
    "graphql": "^0.9.6",
    "graphql-tools": "^0.11.0",
    "moment": "^2.18.1",
    "rimraf": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
